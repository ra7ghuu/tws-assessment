#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/tws/assessments/myapp/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/bin/nopt.js/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/bin/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/nopt@3.0.6/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/node_modules:/home/theia/tws/assessments/myapp/node_modules:/home/theia/tws/assessments/node_modules:/home/theia/tws/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/node-gyp@3.8.0/node_modules/node-gyp/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/node-gyp@3.8.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@3.0.6/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../nopt@3.0.6/node_modules/nopt/bin/nopt.js" "$@"
fi
