#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/tws/assessments/myapp/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/uuid@3.4.0/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/node_modules:/home/theia/tws/assessments/myapp/node_modules:/home/theia/tws/assessments/node_modules:/home/theia/tws/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/request@2.88.2/node_modules/request/node_modules:/home/theia/tws/assessments/myapp/node_modules/.pnpm/request@2.88.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
fi
